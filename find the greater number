
import java.util.Scanner;
class HelloWorld {
    public static void main(String[] args) {
       int a=10;
       Scanner scan =new Scanner(System.in);
       System.out.println("enter your good name please");
       String name=scan.nextLine();
       System.out.println("enter your number and check which number is greater");
       int b=scan.nextInt();
       
       if(a>b){
           System.out.println("opps..!! "+name+ " computer's input is greater then you buddy");
       }
       else{
           System.out.println("Waooo... "+name+" your number is greater then computer's input");
       }
       
    }
}
