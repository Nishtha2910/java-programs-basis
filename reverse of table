import java.util.Scanner;
class HelloWorld {
    public static void main(String[] args) {
        Scanner scan=new Scanner(System.in);
        System.out.println("enter the number");
        int n=scan.nextInt();
        for(int i=10;i>=1;i--){
            System.out.printf("%d X %d =%d \n", n, i, n*i);
            
        }
    }
}
