class HelloWorld {
    public static void main(String[] args) {
    int a=5;
    int b=a++;
    System.out.println("first print : " +b+ " then increment :" +a);
    int d=++a;
    System.out.println("first increment :" +d+" then print :"+a);
    int x=4;
    int y=x--;
    System.out.println("first print the value :"+y+" then decrement :" +x);
    int z=--x;
    System.out.println("first decrement :"+z+ " then print the value :"+x);
    
    
       // increment and drecrement using with operators
    int num=7;
    int num2= ++num + 8;
    System.out.println(num2);
    }
}
